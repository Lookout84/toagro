# API URL для підключення до бекенду
VITE_API_URL=http://localhost:5000/api

# Налаштування для розробки
VITE_DEV_MODE=true

# Налаштування для сервера
VITE_SERVER_PORT=5001

# Ключі для інтеграцій (прикладі, які можна використовувати в майбутньому)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_LIQPAY_PUBLIC_KEY=your_liqpay_public_key

# Налаштування для завантаження зображень
VITE_MAX_UPLOAD_SIZE=5242880 # 5MB в байтах
VITE_ALLOWED_IMAGE_FORMATS=jpg,jpeg,png,webp,gif

# Налаштування для сповіщень
VITE_NOTIFICATION_TIMEOUT=5000 # час відображення сповіщень у мілісекундах